{
  "name": "github-scrapper",
  "version": "1.0.0",
  "description": "This repository is a simple integration with github api to fetch limited information about users",
  "main": "index.ts",
  "scripts": {
    "build": "tsc && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "dev": "nodemon src/index.ts",
    "start": "node dist/index.js",
    "format:check": "prettier --check src",
    "format:write": "prettier --write src",
    "test": "jest \"^.+test\\.ts$\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AllanPazRibeiro/github-scrapper.git"
  },
  "keywords": [
    "github",
    "scrapping",
    "integration",
    "api"
  ],
  "author": "Allan Ribeiro",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AllanPazRibeiro/github-scrapper/issues"
  },
  "homepage": "https://github.com/AllanPazRibeiro/github-scrapper#readme",
  "dependencies": {
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "joi": "^17.11.0",
    "reflect-metadata": "^0.1.13",
    "ts-node-register": "^1.0.0",
    "tsconfig-paths": "^4.2.0",
    "tsyringe": "^4.8.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "ts-jest": "^29.1.1",
    "tscpaths": "0.0.9",
    "typescript": "^5.3.2"
  }
}
